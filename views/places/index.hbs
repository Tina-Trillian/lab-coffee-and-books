<!DOCTYPE html>
<html>
  <head>
    <title>The Best Places on Earth</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <div class="container">
    </div>
      <div class="info">
        <h3>List all places</h3>
        <a href="/new">Create new places</a>
        <ul>
          {{#each places}}
            <li>
              <a href="/{{ this._id}}">
                {{ this.name}}<span>{{ this.description }}</span>
              </a>
               |
              <a href="/{{ this._id}}/edit">Edit</a>
               |
              <a href="/{{ this._id}}/delete">Delete</a>
            </li>
          {{/each }}
        </ul>
      </div>
      <div id="map"></div>
  </body>
  <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBw_pHHP4Yj-9mcD-2ck8o-QjAYnseWyBo"></script>
  <script type="text/javascript" src="/javascripts/main.js"></script>
</html>